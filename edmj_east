let req = new Request('https://edmj.beck.space/east.jpg');
let image = await req.loadImage();
let widget = new ListWidget();
let image2 = widget.addImage(image);
image2.applyFillingContentMode()
image2.centerAlignImage()
image2.imageSize = new Size(338, 158)
Script.setWidget(widget);
QuickLook.present(image)
Script.complete()

